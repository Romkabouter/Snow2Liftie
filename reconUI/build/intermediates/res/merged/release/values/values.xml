<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- From: file:/Users/paulromkes/Documents/Projects/AndroidStudio/Snow2Liftie/reconUI/src/main/res/values/color.xml -->
    <eat-comment/>
    <color name="recon_jet_background">#000000</color>
    <color name="recon_jet_background_translucent">#B3000000</color>
    <color name="recon_jet_gray">#808080</color>
    <color name="recon_jet_highlight">#ffb300</color>
    <color name="recon_jet_text_primary">#FFFFFF</color>
    <color name="recon_jet_text_secondary">#808080</color>
    <color name="recon_jet_white">#FFFFFF</color>
    <declare-styleable name="ButtonActionView"><attr format="string" localization="suggested" name="actionText"/><attr format="reference" name="buttonIcon"/><attr name="buttonType"><enum name="select" value="0"/><enum name="back" value="1"/><enum name="up" value="2"/><enum name="down" value="3"/><enum name="updown" value="4"/></attr></declare-styleable>
    <declare-styleable name="CarouselViewPager"><attr format="boolean" name="showBreadcrumbs"/><attr format="boolean" name="animateSelection"/><attr format="dimension" name="pageMargin"/></declare-styleable>
    <declare-styleable name="ReconListView"><attr format="dimension" name="listHeaderMargin"/></declare-styleable>
    <declare-styleable name="ReconTextView"><attr format="reference" name="fontResource"/></declare-styleable>
    <!-- From: file:/Users/paulromkes/Documents/Projects/AndroidStudio/Snow2Liftie/reconUI/src/main/res/values/themes.xml -->
    <eat-comment/>
    <style name="ReconTheme" parent="android:Theme.NoTitleBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/recon_jet_background</item>
        <item name="android:colorBackground">@color/recon_jet_background</item>
        <item name="android:dialogTheme">@style/ReconTheme.Dialog</item>
        <item name="android:textViewStyle">@style/recon_jet_text_primary</item>
        <item name="android:listViewStyle">@style/recon_list</item>
        <item name="listHeaderMargin">18px</item>
    </style>
    <style name="ReconTheme.Dialog" parent="ReconTheme">
        <item name="android:windowBackground">@color/recon_jet_background_translucent</item>
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/recon_dialog_animation</item>
    </style>
    <style name="ReconTheme.Fullscreen" parent="ReconTheme">
        <item name="android:windowBackground">@color/recon_jet_background</item>
        <item name="android:colorBackground">@color/recon_jet_background</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <!-- From: file:/Users/paulromkes/Documents/Projects/AndroidStudio/Snow2Liftie/reconUI/src/main/res/values/styles.xml -->
    <eat-comment/>
    <style name="recon_button_action">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">match_parent</item>
	</style>
    <style name="recon_button_action_bar">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">38dp</item>
		<item name="android:layout_alignParentBottom">true</item>
		<item name="android:orientation">horizontal</item>
		<item name="android:layout_marginLeft">24dp</item>
		<item name="android:layout_marginBottom">20dp</item>
	</style>
    <style name="recon_dialog_animation">
		<item name="android:windowEnterAnimation">@anim/fade_in_slide_in_bottom</item>
		<item name="android:windowExitAnimation">@anim/fade_out</item>
	</style>
    <style name="recon_horizontal_divider">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">1dp</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:layout_marginBottom">5dp</item>
		<item name="android:background">@android:color/white</item>
	</style>
    <style name="recon_jet_text_highlight" parent="recon_jet_text_primary">
		<item name="android:textColor">@color/recon_jet_highlight</item>
	</style>
    <style name="recon_jet_text_primary">
		<item name="android:textColor">@color/recon_jet_text_primary</item>
		<item name="android:textSize">30dp</item>
		<item name="android:ellipsize">end</item>
	</style>
    <style name="recon_jet_text_secondary" parent="recon_jet_text_primary">
		<item name="android:textColor">@color/recon_jet_text_secondary</item>
	</style>
    <style name="recon_jet_text_selectable" parent="recon_jet_text_primary">
		<item name="android:textColor">@drawable/selectable_text_highlight_color</item>
	</style>
    <style name="recon_list">
		<item name="android:divider">@null</item>
		<item name="android:dividerHeight">-3dp</item>
		<item name="android:fadingEdgeLength">20dp</item>
		<item name="android:listSelector">@android:color/transparent</item>
		<item name="android:requiresFadingEdge">vertical</item>
		<item name="android:scrollbars">none</item>
	</style>
</resources>